{
    "name": "zendframework2 with biso",
    "description": "Zend 2 with Domain Layer",
    "license": "BSD-3-Clause",
    "keywords": ["framework","zf2","ddd","domain"],
    "homepage": "https://github.com/pvgomes/zend2biso/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*",
        "doctrine/doctrine-orm-module": "0.7.*",
        "zendframework/zend-developer-tools": "dev-master",
        "predis/predis": "1.1.*@dev",
        "biso": "dev-master"
    },
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "biso",
          "version": "dev-master",
          "source": {
            "url": "https://github.com/pvgomes/biso",
            "type": "git",
            "reference": "origin/master"
          },
          "autoload": {
            "psr-0": { "Domain\\": "src" }
          }
        }
      }
    ]
}
